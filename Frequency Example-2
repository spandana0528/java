import java.util.*;
public class FrequencyExample2{
    public static void main(String[] args) {
      int[] a={3,2,1,2,2,1,3,1,5,5,4,1};
      int n=a.length;
      int[]freq=new int[10];
      for(int i:a){
          freq[i]++;
      }
      for(int i=0;i<10;i++){
          if(freq[i]>0)
          System.out.print(i+""+freq[i]);
      }
    }
}
      
