import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
      int[] a={3,2,1,2,2,1,3,1,5,5,4,1};
      int n=a.length;
      for(int i=0;i<n;i++)
      {
         
          int count=0;
          for(int j=0;j<n;j++)
          {
              if(i==a[j])
              {
                  count++;
                 
              }
          }
          if(count>0)
          System.out.print(i+""+count);
      }
    }
}

    
