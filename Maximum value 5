import java.io.*;
import java.util.*;
public class FindMaxInArray {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int max = Integer.MIN_VALUE;
        for (int i = 0; i < n; i++) {
            int number = scanner.nextInt();
            if (number > max) {
                max = number;
            }
        }
        System.out.println(max);
    }
}
